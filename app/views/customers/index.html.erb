<p id="notice"><%= notice %></p>

<h1>Customers</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= "#{customer.first_name} #{customer.last_name}" %></td>
        <td><%= customer.address %></td>
        <td><%=  %></td>
        <td><%= link_to 'Show', customer_path(customer) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

